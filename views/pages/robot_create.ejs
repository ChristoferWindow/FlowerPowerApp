<form action="/plant" method="post">
    <div class="form-group">
        <input class="form-control" id="image" type="text" name="image" placeholder="Link do zdjęcia">
    </div>
    <div class="form-group">

        <input class="form-control" id="robotId" type="text" name="robotId" placeholder="Urządzenie">
    </div>
    <div class="form-group">

        <input class="form-control" id="port_color" type="text" name="port_color" placeholder="Kolor portu">
    </div>
    <div class="form-group">

        <input class="form-control" id="name" type="text" name="name" placeholder="Nazwa rośliny">
    </div>
    <div class="form-group">

        <input class="form-control" id="place" type="text" name="place" placeholder="Miejsce położenia roślinki">
    </div>
    <div class="form-group">

        <input class="form-control" id="type" type="text" name="type" placeholder="Gatunek/typ">
    </div>
    <div class="form-row">
        <div class="col">
            <input class="form-control" id="min_water" type="text" name="min_water" placeholder="min. nawodnienie">
        </div>
        <div class="col">
            <input class="form-control" id="max_water" type="text" name="max_water" placeholder="max. nawodnienie">
        </div>
    </div>
    <input type="hidden" name="_csrf" value="<%- _csrf %>" />

    <input id="create_button" class="btn btn-primary text-center" type="submit" placeholder="Stwórz">
</form>
