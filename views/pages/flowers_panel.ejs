<div id="body-container">
    <div id="flowers_panel">
        <% if(plants) { %>
            <% plants.forEach( plant => { %>
                <a href="/web/flowers/<%= plant.id %>">
                    <div id="plant_<%= plant.id %>">
                        <div id="plant_panel">
                            <img src="<%= plant.image %>" class="plant_image"><br/>
                            Nazwa: <%= plant.name %><br/>
                            Typ: <%= plant.type %>
                            <hr>
                        </div>
                    </div>
                </a>
            <% }) %>
        <% } else { %>
           No plants found
        <% }%>
    </div>
</div>
